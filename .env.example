GEMINI_API_KEY=your-api-key-here
GEMINI_MODEL=gemini-2.0-flash
GEMINI_TEMPERATURE=0.7
GEMINI_MAX_TOKENS=2048

# Logging configuration (uncomment to enable)
#GEMINI_LOG_ENABLED=true
#GEMINI_LOG_TYPE=both                    # Options: fifo, file, both
#GEMINI_LOG_PATH=./logs/gemini-repl.log
#GEMINI_LOG_FIFO=/tmp/gemini.fifo
#GEMINI_LOG_FORMAT=json
#GEMINI_LOG_LEVEL=debug                  # Options: info, debug

# Advanced logging settings (optional)
#GEMINI_LOG_ROTATE_SIZE=10M
#GEMINI_LOG_ROTATE_COUNT=5
#GEMINI_LOG_BUFFER_SIZE=1000
#GEMINI_LOG_FLUSH_INTERVAL=100

# Sanitization settings (optional)
#GEMINI_LOG_SANITIZE_API_KEYS=true
#GEMINI_LOG_SANITIZE_PII=false
